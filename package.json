{
  "name": "dashad-project",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node backend/index.js",
    "dev": "nodemon backend/index.js",
    "api:test": "newman run postman/caria_api.postman_collection.json -e postman/caria_local.postman_environment.json --reporters cli",
