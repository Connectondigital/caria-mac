    "mongoose": "^7.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "newman": "^6.2.1",
    "newman-reporter-htmlextra": "^1.23.1",
    "nodemon": "^3.0.0"
